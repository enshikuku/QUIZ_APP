<%- include('partials/header')%>
    
    <main id="login-form">
        <form action="/admin/login" method="post">
            <h1>Log In as Teacher</h1>
            <h1>Log In</h1>
            <% if(error) { %>
                <p class="error"><%= message %></p>
            <% } %>
            <input type="email" name="adminEmail" id="" value="" placeholder="Email Address" required>
            <input type="password" name="adminPassword" id="" value="" placeholder="Password" required minlength="8">
            <input type="submit" value="Log In">
        </form>
    </main>


<%- include('partials/footer')%>